<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
   default-init-method="init">

    <import resource="NetexImportContext.xml"/>
    <import resource="NetexExportContext.xml"/>

    <bean id="NetexPluginInjector" class="fr.certu.chouette.plugin.PluginInjector"
		factory-method="createLinePluginInjector">
        <property name="manager" ref="lineManager" />
        <property name="exportPlugins">
            <list>
                <ref bean="netexLineExport" />
            </list>
        </property>
        <property name="importPlugins">
            <list>
                <ref bean="netexLineImport" />
            </list>
        </property>
    </bean>

</beans>
