<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
   default-init-method="init">


    <bean id="netexLineExport" class="fr.certu.chouette.exchange.netex.exporter.NetexExportPlugin">
       <property name="netexFileWriter" ref="netexFileWriter"/>    
    </bean>
    
    <bean id="netexFileWriter" class="fr.certu.chouette.exchange.netex.exporter.NetexFileWriter">
       <property name="velocityEngine" ref="velocityEngine" />
    </bean>	
  
    <bean id="velocityEngine" class="org.springframework.ui.velocity.VelocityEngineFactoryBean">
        <property name="configLocation" value="classpath:velocity.properties" />
    </bean>

</beans>
    